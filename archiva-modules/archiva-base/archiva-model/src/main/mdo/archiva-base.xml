        </codeSegment>
        <codeSegment>
          <version>1.0.0+</version>
          <code><![CDATA[
    private static String defaultString( String value )
    {
        if ( value == null )
        {
            return "";
        }
        
        return value.trim();
    }
          
    public static String toKey( ProjectReference reference )
    {
        StringBuffer key = new StringBuffer();

